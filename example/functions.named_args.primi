//
// Calling functions with named arguments.
//

function something(a, b, c) {
	return [a, b, c]
}

result = something(10, c: 20, b: 50)
assert(result == [10, 50, 20])

result = something(c: 3, a: 2, b: 1)
assert(result == [2, 1, 3])

result = something(a: 4, b: 5, c: 6)
assert(result == [4, 5, 6])
