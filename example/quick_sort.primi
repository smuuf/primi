function qsort_wrapper(arr) {

    low = 0
    high = arr.length() - 1
    qsort(arr, low, high)

}

function qsort(arr, low, high) {

    if (low < high) {
        pi = partition(arr, low, high)
        qsort(arr, low, pi - 1)
        qsort(arr, pi + 1, high)
    }

}

function partition(arr, low, high) {

    pivot = arr[high]
    i = (low - 1)
    j = low
    while (j <= high- 1) {
        if (arr[j] <= pivot) {
            i = i + 1
            tmp_a = arr[i]
            tmp_b = arr[j]
            arr[i] = tmp_b
            arr[j] = tmp_a
        }
        j = j + 1
    }

    tmp_a = arr[i + 1]
    tmp_b = arr[high]
    arr[i + 1] = tmp_b
    arr[high] = tmp_a

    return i + 1

}

