		"smuuf/better-php-exceptions": "^0",
		"symfony/polyfill-php80": "^1.20"
	},
	"require-dev": {
		"smuuf/phpcb": "dev-master",
		"nette/tester": "^2.0",
		"phpstan/phpstan": "^0.12"
	}
}
