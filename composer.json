{
	"name": "smuuf/primi",
	"description": "A scripting language written in PHP & executable within PHP.",
	"license": "MIT",
	"type": "library",
	"authors": [
		{
			"name": "Premysl Karbula",
			"email": "premavansmuuf@gmail.com"
		}
	],
	"support": {
		"issues": "https://github.com/smuuf/primi/issues",
		"source": "https://github.com/smuuf/primi"
	},
	"autoload":{
		"psr-4": {
			"Smuuf\\Primi\\": "src/"
		}
	},
	"require": {
		"php": ">=7.4",
		"ext-bcmath" : "*",
		"ext-mbstring" : "*",
